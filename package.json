{
  "homepage": "https://natixe.github.io/Boutique-NoName",
  "name": "boutique-noname",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "server-dev": "nodemon server/server.js",
    "server-prod": "node server/server.js",
    "client-dev": "vite",
    "start": "concurrently \"npm run server-dev\" \"npm run client-dev\" --host 0.0.0.0",
    "start-prod": "concurrently \"npm run server-prod\" \"npm run client-dev\" --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npx prettier --write **/*.{js,jsx,md}",
    "format:check": "npx prettier --check **/*.{js,jsx,md}",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "vite": "^5.4.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "gh-pages": "^6.2.0",
    "globals": "^15.12.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
